{
  "consumes": [
    "multipart/form-data",
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "http",
    "https"
  ],
  "swagger": "2.0",
  "info": {
    "description": "Package DahSing PGP files upload API",
    "version": "1.0.1"
  },
  "host": "API_HOST",
  "basePath": "/",
  "paths": {
    "/encrypt": {
      "post": {
        "description": "Encrypt source file to PGP",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json",
          "text/plain; charset=utf-8"
        ],
        "operationId": "encrypt",
        "parameters": [
          {
            "type": "file",
            "description": "The file to upload.",
            "name": "upload",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "format": "textarea",
            "description": "PGP public key",
            "name": "key",
            "in": "formData",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Error"
          }
        }
      }
    },
    "/upload": {
      "post": {
        "description": "Encrypt source file to PGP and Upload to SFTP",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "operationId": "upload",
        "parameters": [
          {
            "type": "file",
            "description": "The file to upload.",
            "name": "upload",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "format": "textarea",
            "description": "PGP public key",
            "name": "key",
            "in": "formData",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Error"
          }
        }
      }
    }
  },
  "responses": {
    "ServiceResult": {
      "schema": {
        "type": "object"
      },
      "headers": {
        "data": {},
        "error": {
          "type": "string"
        },
        "status": {
          "type": "boolean"
        }
      }
    }
  }
}