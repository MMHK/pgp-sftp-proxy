{
    "listen": "${HOST}",
    "tmp_path": "${TEMP}",
	"web_root" : "${ROOT}",
	"ssh" : {
		"host" : "${SSH_HOST}",
		"user" : "${SSH_USER}",
		"password" : "${SSH_PWD}",
		"key" : "${SSH_KEY}"
	},
	"sftp": {
		"upload-dir": "${SFTP_UPLOAD_DIR}",
		"download-dir": "${SFTP_DOWNLOAD_DIR}"
	},
	"pgp": {
		"public-key": "${PGP_PUBLIC_KEY}",
		"private-key": "${PGP_PRIVATE_KEY}"
	},
	"time-range": [
		{
			"begin": "00:00",
			"end": "18:00"
		},
		{
			"begin": "22:00",
			"end": "00:00"
		}
	]
}