{
    "listen": "${HOST}",
    "tmp_path": "${TEMP}",
	"web_root" : "${ROOT}",
	"ssh" : {
		"host" : "${SSH_HOST}",
		"user" : "${SSH_USER}",
		"password" : "${SSH_PWD}",
		"key" : "${SSH_KEY}"
	},
	"sftp": {
		"upload-dir": "${SFTP_UPLOAD_DIR}",
		"download-dir": "${SFTP_DOWNLOAD_DIR}"
	},
	"pgp": {
		"public-key": "${PGP_PUBLIC_KEY}",
		"private-key": "${PGP_PRIVATE_KEY}"
	},
	"aws": {
		"region": "${AWS_REGION}",
		"access-key": "${AWS_ACCESS_KEY}",
		"secret-key": "${AWS_ACCESS_SECRET}",
		"bucket": "${AWS_BUCKET}"
	},
	"policy-words": {
		"private-motor": "${POLICY_WORDS_PRIVATE_MOTOR}",
		"commercial-motor": "${POLICY_WORDS_COMMERCIAL_MOTOR}"
	},
	"webhook":{
		"apikey": "${WEBHOOK_APIKEY}",
		"upload": "${WEBHOOK_UPLOAD_ENDPOINT}",
		"inquire": "${WEBHOOK_INQUIRE_ENDPOINT}"
	},
	"time-range": [
		{
			"begin": "17:55",
			"end": "22:00"
		},
		{
			"begin": "00:00",
			"end": "09:30"
		}
	]
}